{
  "diagnostic.displayByAle": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.refreshAfterSave": true,
  "coc.preferences.messageLevel": "error",
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.formatOnSaveFiletypes": ["go", "rust", "css", "javascript", "typescript", "typescriptreact"],
  "python.analysis.diagnosticEnabled": false,
  "python.linting.enabled": false,
  "python.jediEnabled": false,
  "rust-analyzer.serverPath": "/usr/bin/rust-analyzer",
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.autoFix": true,
  "go.goplsOptions": {
    "usePlaceholders": true,
    "completeUnimported": true
  }
}
